<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Dream_carMVC</title>
    <!-- <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
     <link rel="stylesheet" href="~/css/site.css" />-->

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:400,600" /><!--all style data from CSS template-->
    <link rel="stylesheet" href="~/css/all.min.css" /><!--all style data from CSS template-->
    <link rel="stylesheet" href="~/css/bootstrap.min.css" /><!--all style data from CSS template-->
    <link rel="stylesheet" href="~/slick/slick.css" /><!--all style data from CSS template-->
    <link rel="stylesheet" href="~/slick/slick-theme.css" /><!--all style data from CSS template-->
    <link rel="stylesheet" href="~/css/magnific-popup.css"><!--all style data from CSS template-->
    <link rel="stylesheet" href="~/css/templatemo-dream-pulse.css" /><!-- i have all the style from the CSS template-->
</head>
<body>
    <main class="container-fluid">
        <div class="row">
            <nav id="tmSidebar" class="tm-bg-black-transparent tm-sidebar">
                <button class="navbar-toggler" type="button" aria-label="Toggle navigation">
                    <i class="fas fa-bars"></i>
                </button>
                <div class="tm-sidebar-sticky">
                    <div class="tm-brand-box">
                        <div class="tm-double-border-1">
                            <div class="tm-double-border-2">
                                <h1 class="tm-brand text-uppercase">Dream Car</h1>
                            </div>
                        </div>
                    </div>

                    <ul id="tmMainNav" class="nav flex-column text-uppercase text-right tm-main-nav">
                        <li class="nav-item">
                            <a asp-area="" asp-controller="Companies" asp-action="Index" class="nav-link active">
                                <!--linking of data from the company index file to nav bar-->
                                <span class="d-inline-block mr-3">Company</span>
                                <span class="d-inline-block tm-white-rect"></span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-area="" asp-controller="Dealers" asp-action="Index" class="nav-link">
                                <!--linking of data from the dealer index file to nav bar-->
                                <span class="d-inline-block mr-3">Distributor</span>
                                <span class="d-inline-block tm-white-rect"></span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-area="" asp-controller="Models" asp-action="Index" class="nav-link">
                                <!--linking of data from the model index file to nav bar-->
                                <span class="d-inline-block mr-3">Models</span>
                                <span class="d-inline-block tm-white-rect"></span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-area="" asp-controller="Specifications" asp-action="Index" class="nav-link">
                                <!--linking of data from the specification index file to nav bar-->
                                <span class="d-inline-block mr-3">Specification</span>
                                <span class="d-inline-block tm-white-rect"></span>
                            </a>
                        </li>

                    </ul>
                    <ul class="nav flex-row tm-social-links">
                        <partial name="_LoginPartial" /><!--this code to add login and register to site to get access/change (make all data private)-->

                    </ul>
                    <footer class="text-center text-white small">
                        <p class="mb--0 mb-2"> 2020 Dream Car</p>
                        <p class="mb-0">
                            Design: Bhawandeep Singh
                            <a rel="nofollow" href="" class="tm-footer-link">Cars</a>
                        </p>
                    </footer>
                </div>
            </nav>


            <main role="main" class="ml-sm-auto col-12">
                <div class="parallax-window"
                     data-parallax="scroll"
                     data-image-src="img/home.jpeg">
                    <div class="tm-section-wrap">
                        <section id="intro" class="tm-section">
                            <div class="tm-bg-white-transparent tm-intro">
                                <h2 class="tm-section-title mb-5 text-uppercase tm-color-primary"> Dream Cars</h2><!--headiing-->
                                <p class="tm-color-gray">
                                    Absolutely Never
                                    Give Up On Your
                                    Dreams Because,
                                    Others May
                                    Not Know Just
                                    How Big Your
                                    Dreams Really
                                    Are.
                                </p><!--all data must in paragragh-->
                                <p class="mb-0 tm-color-gray">
                                    Don't Tell People Yours Dreams. <b><i>Show Them!</i></b>
                                </p><!--all data must in paragragh-->
                            </div>
                        </section>
                    </div>
                </div>
                <div class="tm-section-wrap bg-white">
                    <section id="work" class="row tm-section">
                        <div class="col-12">
                            <div class="w-100 tm-double-border-1 tm-border-gray">
                                <div class="tm-double-border-2 tm-border-gray tm-box-pad">
                                    <div class="tm-gallery-wrap">


                                        @RenderBody()<!--getting all data from renderbody i.e home index in shared folder-->
                                    </div>
                                </div>
                            </div>

                        </div>
                    </section>
                </div>
            </main>
            <script src="~/js/jquery.min.js"></script>
            <script src="~/js/jquery.singlePageNav.min.js"></script>
            <script src="~/js/parallax.min.js"></script>
            <script src="~/slick/slick.min.js"></script>
            <script src="~/js/jquery.magnific-popup.min.js"></script><!--all J queries  from the template which are suitable for  this website-->


            @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
